<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_enabled="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/edit_ash_ed" />
            <corners android:radius="1000dp" />
            <stroke android:color="@color/edit_ash_ed_border" android:width="2dp"/>
        </shape>
    </item>

    <item android:state_enabled="false">
        <shape android:shape="rectangle">
            <solid android:color="@color/edit_ash_ed_disabled" />
            <corners android:radius="1000dp" />
            <stroke android:color="@color/edit_ash_ed_disabled" android:width="2dp"/>
        </shape>
    </item>

</selector>